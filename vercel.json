{
  "version": 2,
  "builds": [
    {
      "src": "packages/api/src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "packages/api/src/main.ts"
    }
  ],
  "functions": {
    "packages/api/src/main.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
} 